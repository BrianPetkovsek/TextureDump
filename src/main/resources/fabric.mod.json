{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Texture Dump",
  "description": "Texture Map Dumper for Minecraft Fabric",
  "authors": ["mezz", "LexManos", "AtomicBlom", "BrianPetkovsek"],
  "contact": {
    "homepage": "https://github.com/BrianPetkovsek/TextureDump",
    "sources": "https://github.com/BrianPetkovsek/TextureDump"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.fabricmc.texturedump.TextureDump"
    ]
  },
  "mixins": [
    "modid.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
